<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Indexr - @ViewBag.Title</title>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <meta name="description" content="Description of your web page goes here." />
    <meta name="keywords" content="Keywords for you web page go here. Each keyword or group of keyword phrases are separated by a comma. Keep this keyword list short and relevant to the content of this specific page and also relevant to the tile of the specific page" />
    <meta name="author" content="Author for you page go here." />
    <meta name="viewport" content="width=device-width" />
    <!-- Always force latest IE rendering engine (even in intranet) & Chrome Frame -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href="~/apple-touch-icon.png" rel="apple-touch-icon" type="image/x-icon" />
    <!-- Google Web fonts -->
    <link href='http://fonts.googleapis.com/css?family=Quattrocento:400,700' rel='stylesheet'
        type='text/css' />
    <link href='http://fonts.googleapis.com/css?family=Patua+One' rel='stylesheet' type='text/css' />
    <link href="@Url.Content("~/App_Themes/Default/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/App_Themes/Default/first.theme.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/App/jquery.ui.theme.custom.css")" rel="stylesheet" type="text/css" />
    @*<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>*@
    @*<script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>*@
    <script src="@Url.Content("~/Scripts/jquery-1.8.3.min.js")" type="text/javascript"></script>
    @*<script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>*@
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.24.min.js")" type="text/javascript"></script>
    @*<script src="@Url.Content("~/Scripts/jquery-ui-1.9.2.min.js")" type="text/javascript"></script>*@
    @*<script src="@Url.Content("~/Scripts/jquery-ui.min.js")" type="text/javascript"></script>*@
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.6.2.js")" type="text/javascript"></script>
    <link rel="stylesheet" href="@Url.Content("~/Content/Kendo/kendo.common.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/Kendo/kendo.default.min.css")" />
    <script src="@Url.Content("~/Scripts/Kendo/kendo.web.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Kendo/kendo.aspnetmvc.min.js")" type="text/javascript"></script>
</head>
<body class="page">
    <header class="onerow color2">
		<div class="onepcssgrid-1200">
			<div class="col4 iconic">
				<div class="title">
					<a href="#">Index<span>r</span></a>
				</div>
				<div>Personal contact index</div>
			</div>
            <nav class="col8 last">
                <ul id="mainnav">
                    <li class="active">
                        @Html.ActionLink("Home", "Index", "Home")
                    </li>
                </ul>
            </nav>
			<div class="arrow"></div>
		</div>
    </header>
    <section class="onerow full color1">
        <div class="onepcssgrid-1200">
            @RenderBody()
        </div>
	</section>
    <section class="onerow full color3">
		<div class="onepcssgrid-1200">
			<div class="col4 iconic icon-twitter-zone">
				<h1 class="ic">Latest <span>tweets</span></h1>
                @RenderSection("myTweets", required: false)
				<a class="btn-small" href="http://twitter.com/raselbappi">Follow me on Twitter</a>
			</div>

			<div class="col4 iconic icon-blog-zone">
				<h1 class="ic">Latest <span>blog posts</span></h1>
                @RenderSection("myBlogs", required: false)
				<a class="btn-small" href="http://springsolution.net/">Visit me on Spring Solution</a>
			</div>

			<div class="col4 last iconic icon-site-zone">
				<h1 class="ic">Latest <span>projects</span></h1>
                @RenderSection("myProjects", required: false)
                <a class="btn-small" href="http://drirro.com/">Hire me on Drirro</a>
			</div>
		</div>
	</section>
    <footer class="onerow full color2">
		<div class="onepcssgrid-1200">
			<nav class="col6">
				<ul class="social">
					<li><a class="icon-facebook" href="#"><span>facebook</span></a></li>
					<li><a class="icon-twitter" href="#"><span>twitter</span></a></li>
					<li><a class="icon-google-plus" href="https://plus.google.com/u/0/116968312883081845518"><span>googleplus</span></a></li>
                    <li><a class="icon-linkedin" href="#"><span>linkedin</span></a></li>
                    <li><a class="icon-github" href="#"><span>github</span></a></li>
				</ul>
			</nav>

			<div class="col6 last">
				<p class="right">Indexr &copy; @DateTime.Now.Year</p>
			</div>
		</div>
	</footer>
    @RenderSection("appScripts", required: false)
</body>
</html>
